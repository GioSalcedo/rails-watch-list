<h1>Lista</h1>
<% @lists.each do |list| %>
<div class="container">
  <div class="row justify-content-start">
    <div class="col-4">
      <%= link_to list.name, edit_list_path(list), class: "nav-item nav-link active" %>
    </div>
  </div>
</div>
<% end %>


<%= link_to "Add New List", new_list_path, class: "btn btn-secondary" %>
